    <mapper namespace="com.iu.s1.bankbook.BankBookDAO">
    
    <select id="detail" parameterType="com.iu.s1.bankbook.BankBookDTO" resultType="com.iu.s1.bankbook.BankBookDTO">
   		 SELECT * FROM BANKBOOK WHERE BOOKNUMBER = #{bookNumber}
    </select>
    
